var custom_list_controller = {
	hasChildren : function(element){
		return $(element).children().length > 0;
	}
}

<% if @chickens.nil? %>
	$("#chicken-list").append("NÃ£o temos galinha alguma ='(");
<% else %>
	var htmlElement = '#chicken-list';
	if(!custom_list_controller.hasChildren(htmlElement)){
		$(htmlElement).append("<%= escape_javascript(render 'custom_list', chickens: @chickens) %>");
	}
<% end %>